# Function shorthand.
(def @fun (lambda {f b} {
  def (head f) (lambda (tail f) b)
}))

(def @assert (lambda {expr name} {
  if (? (eval expr))
    { } # It's fine. do nothing.
    { error (concat "Assertion failed: " name) }
}))

